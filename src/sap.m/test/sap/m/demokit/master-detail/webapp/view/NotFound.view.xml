<mvc:View
	controllerName="sap.ui.demo.masterdetail.controller.NotFound"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">

	<IllustratedMessage
		id="page"
		title="{i18n>notFoundTitle}"
		description="{i18n>notFoundText}"
		illustrationType="sapIllus-PageNotFound">
		<additionalContent>
			<Button text="Not Found Action" press=".onNavBack" />
		</additionalContent>
	</IllustratedMessage>

</mvc:View>